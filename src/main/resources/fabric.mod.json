  "accessWidener": "aio.accesswidener",
  "depends": {
    "fabricloader": ">=0.18.4",
    "fabric-api": ">=0.141.1",
    "minecraft": ">=1.21.11",
    "java": ">=21"
  },
  "suggests": {
    "sodium": "*",
    "lithium": "*",
    "iris": "*",
