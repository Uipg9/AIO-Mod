  "authors": ["Uipg9"],
  "contact": {
    "sources": "https://github.com/Uipg9/aio-mod"
  },
  "license": "MIT",
  "icon": "assets/aio/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": ["com.baesp.aio.AioMod"],
